PROJECTS = Foto3x4.exe TestServer.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Foto3x4.exe: Foto3x4.dpr
  $(DCC)

TestServer.exe: TestServer.dpr
  $(DCC)


